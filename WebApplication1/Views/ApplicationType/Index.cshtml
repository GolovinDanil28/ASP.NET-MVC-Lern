@model IEnumerable<WebApplication1.Models.ApplicationType>


<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">ApplicationType List</h2>
        </div>
        <div class="col-6">
            <a asp-action="Create" class="btn btn-primary">Create new ApplicationType</a>
        </div>
    </div>
    <br/>
    @if(Model.Count()>0)
    {
        <table class="table table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th>
                         Name
                    </th>
                    <th>
                       
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach(var obj in Model)
                {
                    <tr>
                        <td style="width:50%">
                            @obj.Name
                        </td>
                        <td style="width:30%">
                        </td>
                        <td></td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <p>Not ApplicationType exists.</p>
    }

</div>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

